{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-moz-transform",
          "user-select",
          "@-ms-viewport",
          "@-ms-viewport"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off",
        "aria-valid-attr": "off"
      }
    ],
    "disown-opener": "off",
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 121",
    "not and_ff <= 122",
    "not chrome <= 121",
    "not edge <= 121",
    "not firefox <= 122",
    "not ios_saf <= 17.3",
    "not opera <= 106",
    "not safari <= 17.3",
    "not samsung <= 23"
  ]
}